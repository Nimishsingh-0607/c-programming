#include<stdio.h>
int main()
{
	int a;
	printf("Enter your marks:",a);
	scanf("%d",&a);
	if (a>=85)
		printf("You got a Grade-A");
	else if (a<85 && a>=70)
		printf("You got a Grade-B");
	else if (a<70 && a>=55)
		printf("You got a Grade-C");
	else if (a<55 && a>=40)
		printf("You got a Grade-D");
	else if (a<40)
		printf("You got a Grade-F");
}
